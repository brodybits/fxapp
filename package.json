{
  "name": "fxapp",
  "description": "Build JavaScript apps using effects as data",
  "version": "0.0.1",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fxapp/fxapp.git"
  },
  "files": ["src", "dist"],
  "keywords": ["effects", "data", "vdom"],
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "jest": "^22.0.4"
  },
  "scripts": {
    "lint": "npx eslint {src,test}/**/*.js",
    "test": "jest --coverage --no-cache",
    "format": "npx prettier --write {src,test}/**/*.js"
  },
  "babel": {
    "presets": "env"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "jest": true
    },
    "rules": { "no-use-before-define": "error" }
  },
  "author": "Wolfgang Wedemeyer <wolf@okwolf.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fxapp/fxapp/issues"
  },
  "homepage": "https://github.com/fxapp/fxapp"
}
